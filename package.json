{
  "name": "monarc",
  "productName": "Monarc-browser",
  "version": "1.4.0",
  "description": "Monarc-browser is a lightweight browser coded with Vue and Electron",
  "main": "./dist/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js",
    "build:dll": "webpack --progress --config .electron-vue/webpack.dll.config.js",
    "dev": "node .electron-vue/dev-runner.js",
    "check-types": "tsc -p src/tsconfig.json",
    "pack": "yarn run pack:main && yarn run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --config .electron-vue/webpack.renderer.config.js",
    "test": "yarn run check-types"
  },
  "author": {
    "name": "Boik Su",
    "email": "boik@tdohacker.org"
  },
  "license": "Apache-2.0",
  "repository": "https://github.com/MonarcProject/Monarc-browser",
  "homepage": "https://github.com/MonarcProject/Monarc-browser",
  "dependencies": {
    "collect.js": "^4.28.6",
    "css-element-queries": "^1.2.3",
    "electron-localshortcut": "^3.2.1",
    "electron-process-manager": "^1.0.0",
    "fuse.js": "^6.6.0",
    "idb-keyval": "^6.1.0",
    "nanoid": "^4.0.0",
    "vue": "^2.7.14",
    "vue-electron": "^1.0.6",
    "vue-i18n": "^8.29.2",
    "vue-router": "^3.7.1",
    "vuex": "^3.7.1",
    "element-ui": "^2.15.11",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.0",
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-typescript": "^7.22.0",
    "@babel/plugin-proposal-class-properties": "^7.22.0",
    "@babel/plugin-proposal-decorators": "^7.22.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-vue": "^9.14.0",
    "typescript": "^4.9.5",
    "webpack": "^4.48.0",
    "webpack-cli": "^4.11.0",
    "webpack-dev-middleware": "^4.3.0",
    "webpack-hot-middleware": "^2.27.0",
    "ts-loader": "^9.4.0",
    "cross-env": "^7.0.3",
    "fork-ts-checker-webpack-plugin": "^7.2.14",
    "fork-ts-checker-notifier-webpack-plugin": "^9.0.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "vue-loader": "^15.10.1",
    "vue-template-compiler": "^2.7.14",
    "style-loader": "^3.3.3",
    "mini-css-extract-plugin": "^2.7.6",
    "html-webpack-plugin": "^5.5.3",
    "less": "^4.2.0",
    "less-loader": "^11.1.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
